{
  "expo": {
    "name": "ColorCatalog",
    "description": "Catalog your colors. Save your favourite web colors in a list and view details for each color. Designed by: Jidonu Segun",
    "slug": "ColorCatalog",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/segunLogo.png",
    "splash": {
      "image": "./assets/segunLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "Jidonu.Segun.ColorCatalog",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
